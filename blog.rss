<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:atom="http://www.w3.org/2005/Atom">
    <channel>
        <title>Dennis Kats Devblog</title>
        <link>https://denniskats.dev/blog.html</link>
        <atom:link href="{{ "/blog.rss" | prepend: site.url }}" rel="self" type="application/rss+xml"/>
        <language>en-us</language>
        <description>Code walks and hot developer takes.</description>
        <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
        {% for post in site.posts limit:10 %}
        <item>
            <title>{{post.title}}</title>
            <link>{{post.url}}</link>
            <pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
            <dc:creator>Dennis Kats</dc:creator>
            <description>{{post.description}}</description>
        </item>
        {% endfor %}

    </channel>
</rss>